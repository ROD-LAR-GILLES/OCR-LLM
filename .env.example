# Configuraci√≥n para el entorno de OCR-LLM usando Donut

# Donut Configuration
OCR_ENGINE=donut
MODEL_NAME=naver-clova-ix/donut-base-finetuned-cord-v2
USE_GPU=false
PDF_DPI=200
MAX_OUTPUT_LENGTH=1024
NUM_BEAMS=4
NUM_THREADS=4
TEMPERATURE=0.8

# Paths
TEMP_DIR=/tmp/ocr-llm
INPUT_DIR=pdfs
OUTPUT_DIR=output

# Processing
BATCH_SIZE=5
MAX_RETRIES=3
